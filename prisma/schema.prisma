// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model user {

  id String @id @default(cuid())
  name String
  email String
  cellphone String
  password String
  address String
  createdAt DateTime @default(now())
}